# AI Client 동기화 서비스 환경 변수 템플릿
# 실제 비밀 값은 sync_service.secrets 헬퍼를 통해 keyring에 저장하세요.

# FastAPI 애플리케이션 기본 설정
SYNC_SERVICE_APP_NAME="AI Client Sync Service"
SYNC_SERVICE_API_PREFIX="/api/v1"
SYNC_SERVICE_RELOAD=true

# 인프라 구성
SYNC_SERVICE_REDIS_URL="redis://localhost:6379/0"
SYNC_SERVICE_DATABASE_URL="sqlite:///./data/sync_service.db"

# Keyring 서비스 및 프리셋 키 이름
SYNC_SERVICE_SECRET_SERVICE_NAME="ai-client-sync-service"
SYNC_SERVICE_API_TOKEN_KEY="sync-service-api-token"
SYNC_SERVICE_ADMIN_PASSWORD_KEY="sync-service-admin-password"
